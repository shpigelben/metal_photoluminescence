          RowBox[{"fT", "[", "\[ScriptE]", "]"}]}], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"\[ScriptE]", ",", "0", ",", "Infinity"}], "}"}], ",", 
       RowBox[{"Assumptions", "->", "Assumptions"}]}], "]"}]}], ";"}], "\n", 
